# Copy to .env and adjust as needed. All values can be overridden via ENV_FILE or direct env vars.

# Unique timestamp per full run; leave empty to auto-generate (UTC) in run_tests.sh
TS=

# Results directory (defaults to ./results if empty)
RESULTS_DIR=

# Docker settings
NET_NAME=lab-net
MYSQL_CONTAINER=mysql-server
MYSQL_IMAGE=mysql:8.0.44
MYSQL_CLIENT_IMAGE=mysql:8.0.44
MYSQL_ROOT_PASSWORD=MyPassw0rd!
MYSQL_DB=lab_mysqldump_sim
MYSQL_PORT=3306

# Dump tool images
MYDUMPER_IMAGE=mydumper/mydumper:v0.20.1-2
DUMPLING_IMAGE=pingcap/dumpling:v7.5.1
